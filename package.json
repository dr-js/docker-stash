{
  "private": true,
  "name": "docker-stash",
  "version": "0.0.4-dev.8",
  "description": "A collection of strange functions, for development",
  "author": "dr-js",
  "license": "MIT",
  "repository": "github:dr-js/docker-stash",
  "scripts": {
    "// build =======================": "",
    "build-debian10": "dr-dev --rsl build-debian10-core build-debian10-layer build-debian10-layer-cn",
    "build-debian10-core": "node source/debian10/build-core.js",
    "build-debian10-layer": "npm run build-debian10-layer-node && npm run build-debian10-layer-bin && npm run build-debian10-layer-full",
    "build-debian10-layer-node": "node source/debian10/build-layer.js node",
    "build-debian10-layer-bin": "node source/debian10/build-layer.js bin",
    "build-debian10-layer-full": "node source/debian10/build-layer.js full",
    "build-debian10-layer-cn": "npm run build-debian10-layer-node-cn && npm run build-debian10-layer-bin-cn && npm run build-debian10-layer-full-cn",
    "build-debian10-layer-node-cn": "node source/debian10/build-layer.js node CN",
    "build-debian10-layer-bin-cn": "node source/debian10/build-layer.js bin CN",
    "build-debian10-layer-full-cn": "node source/debian10/build-layer.js full CN",
    "build-ubuntu1804": "dr-dev --rsl build-ubuntu1804-core build-ubuntu1804-layer build-ubuntu1804-layer-cn",
    "build-ubuntu1804-core": "node source/ubuntu1804/build-core.js",
    "build-ubuntu1804-layer": "npm run build-ubuntu1804-layer-node && npm run build-ubuntu1804-layer-bin && npm run build-ubuntu1804-layer-full",
    "build-ubuntu1804-layer-node": "node source/ubuntu1804/build-layer.js node",
    "build-ubuntu1804-layer-bin": "node source/ubuntu1804/build-layer.js bin",
    "build-ubuntu1804-layer-full": "node source/ubuntu1804/build-layer.js full",
    "build-ubuntu1804-layer-cn": "npm run build-ubuntu1804-layer-node-cn && npm run build-ubuntu1804-layer-bin-cn && npm run build-ubuntu1804-layer-full-cn",
    "build-ubuntu1804-layer-node-cn": "node source/ubuntu1804/build-layer.js node CN",
    "build-ubuntu1804-layer-bin-cn": "node source/ubuntu1804/build-layer.js bin CN",
    "build-ubuntu1804-layer-full-cn": "node source/ubuntu1804/build-layer.js full CN",
    "// =============================": "",
    "git-fix-win32-filemode": "git update-index --chmod=+x ./source/debian10/build-script/* ./source/ubuntu1804/build-script/*",
    "docker-clear": "node source/docker-clear.js",
    "docker-push": "node source/docker-push.js",
    "docker-ls": "node source/docker-ls.js",
    "lint": "eslint ./source/",
    "prepack": "echo \"Error: do not pack this package\" && exit 1"
  },
  "engines": {
    "node": ">=12.16",
    "npm": ">=6.14"
  },
  "devDependencies": {
    "@dr-js/core": "0.4.0-dev.5",
    "@dr-js/dev": "0.4.0-dev.3",
    "@dr-js/dev-eslint": "0.4.0-dev.3"
  }
}
