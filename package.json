{
  "private": true,
  "name": "docker-stash",
  "version": "0.0.5-dev.3",
  "description": "A collection of strange functions, for development",
  "author": "dr-js",
  "license": "MIT",
  "repository": "github:dr-js/docker-stash",
  "scripts": {
    "// build =======================": "",
    "build-debian10-all": "dr-dev --rsl build-debian10 build-debian10-cn",
    "build-debian10": "node source/debian10/build.js build-layer.js",
    "build-debian10-cn": "node source/debian10/build.js build-layer.js CN",
    "build-proxy": "DRD --EE \"HTTP_PROXY=${npm_config_proxy-http://127.0.0.1:1080}&HTTPS_PROXY=${npm_config_https_proxy-http://127.0.0.1:1080}&NO_PROXY=127.0.0.1,localhost\" -E --",
    "build-debian10-cn-proxy": "npm run build-proxy -- npm run build-debian10-cn",
    "build-debian10-core-cn-proxy": "npm run build-proxy -- node source/debian10/build-core.js CN",
    "build-debian10-layer-node-cn-proxy": "npm run build-proxy -- node source/debian10/build-layer.js node CN",
    "// =============================": "",
    "git-fix-win32-filemode": "git update-index --chmod=+x ./source/debian10/build-layer-script/*",
    "docker-pre-pull-buildkit": "sudo docker image pull \"$(node -p \"require('./source/debian10/BUILDKIT_SYNTAX.json')\")\"",
    "docker-ls": "node source/docker-ls.js",
    "docker-push-all": "node source/docker-push.js ALL",
    "docker-push": "node source/docker-push.js",
    "docker-push-cn": "node source/docker-push.js CN",
    "docker-prune": "node source/docker-prune.js",
    "lint": "eslint ./source/",
    "prepack": "echo \"Error: do not pack this package\" && exit 1"
  },
  "engines": {
    "node": ">=12.19",
    "npm": ">=6.14"
  },
  "devDependencies": {
    "@dr-js/core": "0.4.1-dev.4",
    "@dr-js/dev": "0.4.1-dev.0",
    "@dr-js/dev-eslint": "0.4.1-dev.0",
    "@dr-js/node": "0.4.1-dev.1"
  }
}
